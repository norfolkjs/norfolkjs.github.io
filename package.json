{
    "name": "norfolkjs.github.io",
    "version": "1.0.0",
    "description": "Norfolk.js Website ===================",
    "main": "index.js",
    "type": "module",
    "engines": {
        "npm": ">=8.0.00",
        "node": ">=18.0.0"
    },
    "scripts": {
        "dev": "nodemon -r dotenv/config --watch \"*\" -e \"*\" src/dev.js",
        "start": "npx http-server build -o",
        "prestart": "npm run build",
        "prebuild": "rm -rf build && mkdir build && cp -R static/. build/",
        "build": "node ./src/build.js --input-type=module",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iambriansreed/norfolkjs.github.io.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/iambriansreed/norfolkjs.github.io/issues"
    },
    "homepage": "https://github.com/iambriansreed/norfolkjs.github.io#readme",
    "devDependencies": {
        "eslint": "^8.54.0",
        "gh-pages": "^6.0.0",
        "handlebars": "^4.7.8",
        "marked": "^10.0.0",
        "nodemon": "^3.0.1"
    },
    "dependencies": {
        "dotenv": "^16.4.1",
        "moment": "^2.30.1"
    }
}
